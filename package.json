{
  "name": "single-spa-svelte",
  "version": "2.1.0",
  "description": "a single-spa plugin for svelte applications",
  "main": "lib/single-spa-svelte.js",
  "scripts": {
    "build": "babel src --out-dir lib --source-maps",
    "test": "BABEL_ENV=test jest",
    "lint": "eslint src",
    "prettier": "prettier --write './**'",
    "prepublishOnly": "npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && yarn lint"
    }
  },
  "files": [
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/single-spa/single-spa-svelte.git"
  },
  "keywords": [
    "single-spa",
    "svelte",
    "microservices"
  ],
  "author": "Bret Little",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/single-spa/single-spa-svelte/issues"
  },
  "homepage": "https://github.com/single-spa/single-spa-svelte#readme",
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@types/jest": "^26.0.14",
    "babel-eslint": "^11.0.0-beta.2",
    "babel-jest": "^26.5.2",
    "eslint": "^7.11.0",
    "eslint-config-important-stuff": "^1.1.0",
    "husky": "^4.3.0",
    "jest": "^26.5.2",
    "jest-cli": "^26.5.2",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.0.2"
  }
}
